{
  "compilerOptions": {
  "target": "esnext",
  "module": "esnext",
  "moduleResolution": "node",
  "strict": true,
  "jsx": "preserve",
  "esModuleInterop": true,
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true, 
  "noEmitOnError": true,
  "incremental": true,
  "resolveJsonModule": true,
  "isolatedModules": true,
  "paths": {
  "@/": ["./src/"]
  }
  },
  "include": [
  "src//*.ts",
  "src//*.vue",
  "vite.config.ts"
  ],
  "exclude": [
  "node_modules"
  ]
}